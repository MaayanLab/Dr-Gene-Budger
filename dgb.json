{
    "id": "dgb",
    "instances": 1,
    "cpus": 1,
    "mem": 2048,
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "maayanlab/dgb:2018",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ]
        }
    },
    "labels": {
       "public": "true" 
    }
}